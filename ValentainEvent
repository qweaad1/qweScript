game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9027.85, 6529.48, -8961.04)
wait(0.2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9023.85, 6529.48, -8960.04)
wait(1)

local player = game.Players.LocalPlayer
player.CameraMode = Enum.CameraMode.LockFirstPerson
wait(0.2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame *= CFrame.Angles(0, math.rad(37), 0)


local character = game.Players.LocalPlayer.Character
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")


local function checkHeight()
    while humanoidRootPart.Position.Y >= 100 do
        wait(0.1) 
    end
    humanoidRootPart.CFrame = CFrame.new(-342.01, 30.93, -1443.57)
end


checkHeight()
wait(4)
loadstring(game:HttpGet("https://raw.githubusercontent.com/evxncodes/mainroblox/main/anti-afk", true))()
